

<%- include("partials/header") %>

<h1>Home</h1>
<p><%= startingContent %></p>

<% posts.forEach(function(post) { %>
  <div class="post">
    <h2><%= post.title %></h2>
    <p><%= post.content %></p>
    <form action="/delete" method="POST">
      <input type="hidden" name="postId" value="<%= post._id %>">
      <button type="submit">Delete</button>
    </form>
  </div>
<% }) %>

<%- include("partials/footer") %>
